{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@packages/api": "1.0.0",
    "@packages/components": "1.0.0",
    "@packages/utilities": "1.0.0",
    "@sentry/react": "^6.15.0",
    "@types/file-saver": "^2.0.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.5",
    "@types/recharts": "^1.8.16",
    "antd": "^4.5.1",
    "axios": "^0.25.0",
    "file-saver": "^2.0.5",
    "focus-trap-react": "^8.8.2",
    "moment": "^2.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "recharts": "^2.1.6",
    "typescript": "~4.4.4"
  },
  "scripts": {
    "start": "HTTPS=true craco start",
    "build": "craco build --profile",
    "test": "craco test  --coverage --watchAll=false",
    "test:watch": "craco test --watch",
    "open:cov": "yarn test --coverage --watchAll=false && opener ./coverage/index.html",
    "eject": "craco eject",
    "clean": "rimraf build",
    "clean:node_modules": "rimraf node_modules",
    "lint:ts": "eslint --max-warnings 0 './src/**/*.{ts,tsx}'",
    "lint": "yarn lint-staged --verbose"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "semi": false
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:ts"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "html",
      "json",
      "text",
      "lcov"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.0",
    "@sentry/cli": "^1.55.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-plugin-import": "^1.13.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "less": "^4.0.0",
    "less-loader": "^7.1.0",
    "lint-staged": "^12.0.2",
    "opener": "^1.5.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "sass": "^1.49.9"
  }
}
